<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
        <meta name="description" content="" />
        <meta name="author" content="" />
        <title>Aerobulla - Super Admin</title>
        <!-- Favicon-->
        <link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
        <!-- Font Awesome icons (free version)-->
        <script src="https://use.fontawesome.com/releases/v6.3.0/js/all.js" crossorigin="anonymous"></script>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
        <!-- Google fonts-->
        <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css" />
        <link href="https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700" rel="stylesheet" type="text/css" />
        <!-- Core theme CSS (includes Bootstrap)-->
        <link href="/estilo_tienda.css" rel="stylesheet" />
        <link href="/forms.css" rel="stylesheet" />
    </head>
    <body id="page-top">
        
        <!-- Navigation-->
        <nav class="navbar navbar-expand-lg navbar-dark fixed-top background-trans" >
            <div class="container">
                <a class="" href="#page-top"><img src="/img_tienda/logos/hasb.png" height="75px" alt="..." /></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                    Menu
                    <i class="fas fa-bars ms-1"></i>
                </button>
                <div class="collapse navbar-collapse" id="navbarResponsive">
                    <ul class="navbar-nav text-uppercase ms-auto py-4 py-lg-0">
                        <li class="nav-item"><a class="nav-link" href="#services">Servicios</a></li>
                        <li class="nav-item"><a class="nav-link" href="/ver_habitaciones">Habitaciones</a></li>
                        <li class="nav-item"><a class="nav-link" href="#portfolio">Tiendas</a></li>
                        <li class="nav-item"><a class="nav-link" href="/itinerario">Itinerario</a></li>
                        
                    </ul>
                </div>
            </div>
        </nav>
        <!-- Masthead-->
        <header class="masthead" style="background-image: url('/uploads/banners/bannerhashport.jpg');">
            <div class="container">
                <div class="masthead-subheading">Bienvenido !</div>
                <div class="masthead-heading text-uppercase">Nombre</div>
                <a class="btn btn-primary btn-xl text-uppercase text-black" href="#services">Por</a>
            </div>
        </header>
        <section class="page-section bg-light" id="portfolio">
            <div class="text-center">
                <h2 class="section-heading text-uppercase">Habitaciones</h2>
                <form action="/buscarTienda" method="POST">
                    <!-- <div class="form-group d-flex mb-4">
                        
                        <input class="form-control p-4 rounded-pill" id="comparacion" name="comparacion" type="text" placeholder="Buscador de tiendas">
                        <button class="btn btn-warning px-4 rounded-circle" type="submit"><i class="bi bi-search fs-4"></i></button>
                    </div> -->
                </form>
                <h3 class="section-subheading text-muted">Habitaciones registradas</h3>
            </div>
            <div class="pt-5"></div>
            <div class="container">

                <div class="row">
                <% results.forEach((results)=>{ %>
                    <div class="col-lg-4 col-sm-6 mb-4">
                        <!-- Portfolio item 1-->
                        <div class="portfolio-item mx-4">
                           
                            <a class="portfolio-link" >
                              <div class="image-container">
                                <img class="" src="/uploads/img_habitaciones/<%= results.image %>" alt="..." height="225px" width="100%"/>
                                <% if(results.estado_habitacion_fk === 1) {%>
                                    <div class="image-text">
                                      <a class="btn btn-primary btn-xl text-uppercase text-black"><%= results.nombre_estado_habitacion %></a>
                                    </div>
                                <%}else{%>
                                    <div class="texto-superpuesto text-center">
                                        <a class="btn btn-danger btn-xl text-uppercase text-black text-center" ><%= results.nombre_estado_habitacion %></a>
                                    </div>
                                <%}%>
                                <div class="row position-absolute top-0 start-100" >
                                    <div class="col-12 text-end">
                                        <% if(results.estado_habitacion_fk === 1) {%>
                                            <a href="/superadmin_eliminar_habitacion/<%= results.id_habitacion  %>" class="btn btn-danger btn-circle translate-middle"><i class="fa-solid fa-xmark text-black text-center"></i></a>
                                        <%}else{%>
                                            <a href="/superadmin_habilitar_habitacion/<%= results.id_habitacion  %>" class="btn btn-success btn-circle translate-middle"><i class="fa-solid fa-trash-can-arrow-up text-center "></i></a>
                                        <%}%>
                                    </div>
                                    <div class="col-12 text-end pt-1">
                                        <a data-bs-toggle="modal" href="#portfolioModal-<%= results.id_habitacion %>"  class="btn btn-info btn-circle translate-middle"><i class="fa-solid fa-pen-to-square text-black text-center"></i></a>
                                    </div>
                                </div>
                              </div>
                            </a>
                            <div class="portfolio-caption">
                              <div class="portfolio-caption-heading"><%= results.numero %></div>
                            </div>
                          </div>
                    </div>
                <% }) %>

                <% results.forEach((results)=>{ %>
                    <div class="portfolio-modal modal fade" id="portfolioModal-<%= results.id_habitacion %>" tabindex="-1" role="dialog" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="close-modal" data-bs-dismiss="modal"><img src="assets/img/close-icon.svg" alt="Close modal" /></div>
                                <div class="container-fluid px-1 py-5 mx-auto">
                                    <div class="row d-flex justify-content-center">
                                        <div class="col-xl-7 col-lg-8 col-md-9 col-11 text-center">
                                            <div>
                                                <h2  class="text-center mb-4">Edicion de habitacion</h2>
                                                <form class="form-card" action="/editHabitacion" method="POST" enctype="multipart/form-data">
                                                    <input type="hidden" name="formType" value="habitacion">
                                                    <input hidden type="text" name="id" id="id" value="<%= results.id_habitacion %>">

                                                    <input type="text" name="id" id="id" value="<%= results.id_habitacion %>">
                                                    <div class="row justify-content-between text-left">
                                                        <div class="form-group col-sm-6 flex-column d-flex"><label class="form-control-label px-3">Numero</label> <input type="text" id="numero" name="numero" value="<%= results.numero %>"> </div>
                                                        <div class="form-group col-sm-6 flex-column d-flex"><label class="form-control-label px-3">Precio por Hora</label> <input type="text" id="precio" name="precio" value="<%= results.precio_hora %>"> </div>
                                                    </div>
                                                    <div class="row justify-content-between text-left">
                                                        <div class="form-group col-sm-6 flex-column d-flex"><label class="form-control-label px-3">Imagen</label> <input class="form-control" type="file" id="imagen" name="imagen" value="<%= results.image %>"></div>
                                                    </div>
                                                    <div class="row justify-content-between text-left">
                                                        <div class="form-group col-sm-6 flex-column d-flex"> <label class="form-control-label px-3">Estado de la habitacion</label>
                                                            <select class="form-control mt-2 py-2" id="estado_habitacion" name="estado_habitacion">
                                                                <% estado.forEach((est) => { %>
                                                                    <option value=" <%= est.id_estado_habitacion %>"> <%= est.nombre_estado_habitacion %></option>
                                                                <% }) %>
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-sm-6 flex-column d-flex"> <label class="form-control-label px-3">Sector de la habitacion</label>
                                                            <select class="form-control mt-2 py-2" id="sector" name="sector">
                                                                <% sector.forEach((sec) => { %>
                                                                    <option value=" <%= sec.id_sector %>"> <%= sec.nombre_sector %></option>
                                                                <% }) %>
                                                            </select>
                                                        </div>

                                                    </div>
                                                    <div class="row justify-content-between text-left">
                                                        <div class="form-group col-12 flex-column d-flex"> <label class="form-control-label px-3">Descripcion</label> <input type="text" id="descripcion" name="descripcion" value="<%= results.descripcion%>"> </div>
                                                        <div class="form-group col-12 flex-column d-flex"> <button type="submit" class="btn btn-primary text-black">Enviar</button> </div>
                                                    </div>  
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <% }) %>
                   
                </div>
            </div>
        </section>



       


        <div class="py-5" hidden>
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-md-3 col-sm-6 my-3">
                        <a href="#!"><img class="img-fluid img-brand d-block mx-auto" src="/img_tienda/logos/microsoft.svg" alt="..." aria-label="Microsoft Logo" /></a>
                    </div>
                    <div class="col-md-3 col-sm-6 my-3">
                        <a href="#!"><img class="img-fluid img-brand d-block mx-auto" src="/img_tienda/logos/google.svg" alt="..." aria-label="Google Logo" /></a>
                    </div>
                    <div class="col-md-3 col-sm-6 my-3">
                        <a href="#!"><img class="img-fluid img-brand d-block mx-auto" src="/img_tienda/logos/facebook.svg" alt="..." aria-label="Facebook Logo" /></a>
                    </div>
                    <div class="col-md-3 col-sm-6 my-3">
                        <a href="#!"><img class="img-fluid img-brand d-block mx-auto" src="/img_tienda/logos/ibm.svg" alt="..." aria-label="IBM Logo" /></a>
                    </div>
                </div>
            </div>
        </div>


        <!-- Footer-->
        <footer class="footer py-4">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-4 text-lg-start">Copyright &copy; Aeropuerto 2023</div>
                    <div class="col-lg-4 my-3 my-lg-0">
                        <a class="btn btn-dark btn-social mx-2" href="#!" aria-label="Twitter"><i class="fab fa-twitter"></i></a>
                        <a class="btn btn-dark btn-social mx-2" href="#!" aria-label="Facebook"><i class="fab fa-facebook-f"></i></a>
                        <a class="btn btn-dark btn-social mx-2" href="#!" aria-label="LinkedIn"><i class="fab fa-linkedin-in"></i></a>
                    </div>
                    <div class="col-lg-4 text-lg-end">
                        <a class="link-dark text-decoration-none me-3" href="#!">Politicas de Privacidad</a>
                        <a class="link-dark text-decoration-none" href="#!">Terminos y Condiciones</a>
                    </div>
                </div>
            </div>
        </footer>

        <!-- Bootstrap core JS-->
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
        <!-- Core theme JS-->
        <script src="/scripts_tienda.js"></script>
        <!-- * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *-->
        <!-- * *                               SB Forms JS                               * *-->
        <!-- * * Activate your form at https://startbootstrap.com/solution/contact-forms * *-->
        <!-- * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *-->
        <script src="https://cdn.startbootstrap.com/sb-forms-latest.js"></script>
    </body>
</html>
