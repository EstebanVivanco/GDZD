        <!-- Masthead-->
        <header class="masthead" style="background-image: url('/uploads/banners/bannerhashport.jpg');">
            <div class="container">
                <div class="masthead-subheading">Bienvenido !</div>
                <div class="masthead-heading text-uppercase">Nombre</div>
                <a class="btn btn-primary btn-xl text-uppercase text-black" href="#services">Por</a>
            </div>
        </header>




        <!-- Portfolio Grid-->
        <section class="page-section bg-light" id="portfolio">
            <div class="container">
                <div class="table-responsive align-middle" >
                    
                  <table id="table" class="table table-striped text-center mt-4">
                    
                        <thead class="table-light" >
                            <tr>
        
                                <th>ID HABITACION</th>
                                <th>NUMERO</th>
                                <th>DESCRIPCION</th>
                                <th>PRECIO</th>
                                <!-- <th>CIUDAD</th> -->
                                <th>SECTOR</th>
                                <!-- <th>CIUDAD</th> -->
                                <th>IMAGEN</th> 
                                <th>ESTADO HABITACION</th> 
        
                            </tr>
                        </thead>
        
                        <tbody>
        
                          <% results.forEach((habitacion)=> { %>
                            
                              <tr>
                              
                                <td><%=  %></td>
                              </tr>
        
                          <%}) %>
        
                        </tbody>
        
                    </table>
                  </div>
                </div>
        </section>


        <div class="py-5" hidden>
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-md-3 col-sm-6 my-3">
                        <a href="#!"><img class="img-fluid img-brand d-block mx-auto" src="/img_tienda/logos/microsoft.svg" alt="..." aria-label="Microsoft Logo" /></a>
                    </div>
                    <div class="col-md-3 col-sm-6 my-3">
