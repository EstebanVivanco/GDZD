<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="/estilo_itinerario.css" rel="stylesheet" />
    
    <title> Itinerario </title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" crossorigin="anonymous">
  </head>
  <body >
   
    
    <div class="container">

        <div class="table-responsive align-middle" >
          <table id="table" class="table table-striped text-center mt-4">
                <thead class="table-light" >
                    <tr>

                        <th>COMPAÃ‘IA</th>
                        <th>VUELO</th>
                        <th>EMBARQUE</th>
                        <th>ORIGEN</th>
                        <!-- <th>CIUDAD</th> -->
                        <th>DESTINO</th>
                        <!-- <th>CIUDAD</th> -->
                        <th>ESTADO</th> 

                    </tr>
                </thead>

                <tbody>

                  <% results.forEach((iti)=> { %>
                    
                      <tr>
                      
                        <td><%= iti.compania_aerea %></td>
                        <td><%= iti.numero_vuelo %></td>
                        <td><%= iti.puerta_embarque %></td>
                        <td><%= iti.aeropuerto_origen %></td>
                        <!-- <td><%= iti.ciudad_origen %></td> -->
                        <td><%= iti.aeropuerto_destino %></td>
                        <!-- <td><%= iti.ciudad_destino%></td> -->
                        <td><%= iti.estado_vuelo %></td>
                        
                      </tr>

                  <%}) %>

                </tbody>

            </table>
          </div>
        </div>
        
        
        
        
        
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>
      </body>
</html>